@model List<DismasDemo.Models.PriceList>

@{
    ViewData["Title"] = "Price List Dashboard";
}

<div>
    <h4>Price List Details</h4>
    <hr />
    <dl class="row">
        <dd class="col-sm-10">
            <table class="table table-hover" id="tblInventory">
                <thead>
                    <tr>
                        <th>Price List</th>
                        <th>Part</th>
                        <th>Sku</th>
                        <th>Description</th>
                        <th>List Price</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </dd>
    </dl>
</div>

@section scripts{
    <script src="~/js/signalr/dist/browser/signalr.js"></script>
    <script src="~/js/priceUpdate.js"></script>
}

